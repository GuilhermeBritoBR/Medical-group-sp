<!DOCTYPE html>
<html>
<head>
    <title>Exames | Medical Group SP</title>
    <link rel="stylesheet" type="text/css" href="styles.css">
</head>

<%- include('header-logado'); %>

<div class="container">


    <table class="table-div-consulta">
        <thead>
            <tr>
                <th>Paciente</th>
                <th>Tipo</th>
                <th>Data</th>
                <th>Horário</th>
                <th>Observações</th>
          
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(row) { %>
                <tr>
                    <td><%= row.nome %></td>
                    <td><%= row.type %></td>
                    <td><%= row.data %></td>
                    <td><%= row.horas %></td>
                    <td><%= row.obs %></td>
                    
                </tr>
            <% }); %>
        </tbody>
    </table>




</div>

<%- include('footer'); %>
</html>